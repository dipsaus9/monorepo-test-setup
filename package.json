{
  "name": "test-tailwind",
  "private": true,
  "version": "0.0.1",
  "repository": "git@github.com:dipsaus9/monorepo-test-setup.git",
  "author": "dipsaus9",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:config": "turbo run --filter=./packages/config/**/* build",
    "build:packages": "turbo run --filter=./packages/**/* build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "cleanup": "rm -rf packages/**/*/node_modules && rm -rf packages/*/dist && rm -rf node_modules && rm -f pnpm-lock.yaml",
    "cy:open": "cypress open --component",
    "cy:run": "cypress run --component"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/rollup-plugin-peer-deps-external": "2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.10.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "tailwindcss": "^3.2.1",
    "turbo": "1.6.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "workspaces": [
    "packages/**",
    "apps/**"
  ]
}
