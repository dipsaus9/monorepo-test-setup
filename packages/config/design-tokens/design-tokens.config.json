{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "TypeScript": {
      "buildPath": "./src/",
      "files": [
        {
          "destination": "main.d.ts",
          "format": "typescript/module-declarations"
        },
        {
          "destination": "main.js",
          "format": "javascript/module"
        }
      ]
    }
  }
}
